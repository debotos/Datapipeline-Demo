{
	"heading": "Users",
	"capabilities": {
		"edit": {
			"enable": true,
			"label": "Edit user"
		},
		"delete": true,
		"add": {
			"enable": true,
			"label": "Add a new user",
			"fields": ["first_name", "last_name", "email", "address", "gender", "permission", "active"],
			"initialValues": {
				"gender": "Male",
				"permission": ["read"],
				"active": false
			}
		},
		"search": {
			"enable": true,
			"fields": ["first_name", "last_name", "email", "ip_address"]
		},
		"download": {
			"enable": true,
			"fields": ["first_name", "last_name", "email", "gender", "ip_address"],
			"props": {
				"note": "Disclaimer Message - This data is property of ezilm and you can not use without permission",
				"sheetName": "Sheet1"
			}
		},
		"filter": {
			"enable": true,
			"fields": ["gender", "active"]
		},
		"setting": {
			"enable": true,
			"props": { "hide": ["ip_address"] }
		},
		"refresh": true,
		"pagination": {
			"enable": true,
			"position": ["topRight", "bottomRight"],
			"pageSize": 10,
			"showSizeChanger": true,
			"pageSizeOptions": [10, 16, 25, 50, 100],
			"showQuickJumper": true
		}
	},
	"columnDefs": [
		{
			"headerName": "First Name",
			"field": "first_name",
			"width": 140
		},
		{
			"headerName": "Last Name",
			"field": "last_name",
			"width": 140
		},
		{
			"headerName": "Address",
			"field": "address",
			"width": 220
		},
		{
			"headerName": "Email",
			"field": "email",
			"width": 220
		},
		{
			"headerName": "IP",
			"field": "ip_address",
			"width": 150,
			"type": ["nonEditableColumn"]
		},
		{
			"headerName": "Gender",
			"field": "gender",
			"width": 100
		},
		{
			"headerName": "Permission",
			"field": "permission",
			"width": 150
		},
		{
			"headerName": "Active",
			"field": "active",
			"width": 100
		},
		{
			"headerName": "Action",
			"field": "action",
			"width": 130,
			"type": ["nonEditableColumn", "nonFilterableColumn", "nonSortableColumn"]
		}
	]
}
